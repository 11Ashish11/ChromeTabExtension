{
    "name": "Chrome Tabs Numbers",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Display the number of open tabs in the Chrome browser.",
    "permissions": [
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "*://*/*",
        "<all_urls>"
    ],
    "icons": {
        "128": "logos/logo.png"
    }
}